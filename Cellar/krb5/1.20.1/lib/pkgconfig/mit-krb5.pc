prefix=/usr/local/Cellar/krb5/1.20.1
exec_prefix=${prefix}
libdir=/usr/local/Cellar/krb5/1.20.1/lib
includedir=${prefix}/include

defccname=KCM:
defktname=FILE:/etc/krb5.keytab
